<?xml version="1.0" encoding="utf-8"?>
<Components>
  <!-- TODO 拆分 Objects Connections Initializations 目录 -->
  <!--
    Components: 组件列表
      每个子对象为一个Component组件，对象名为组件名
        组件的子节点，会被替换到所有配置文件中，对应Component属性的位置
        组件替换时，不会覆盖对应位置的同名内容
        组件配置中可以嵌套引用Component
        组件子节点也可以为Array数组，此时仅可用于保存Connections
  -->
  <Objects>
    <Component name="test">
      <Class class="Test" name="test" subThread="true">
        <PubEvent signal="testSignal(quint32)" topic="TestEvent"/>
        <PubEvent eventId="TestEvent" topic="TestEvent"/>
        <SubEvent eventId="TestSlot" topic="TestEvent"/>
      </Class>
    </Component>

    <Component name="mainWindow">
      <Class class="MainWindow" name="mainWindow">
<!--        <CentralWidget class="LogDisplayWidget">-->
<!--        </CentralWidget>-->
        <SubEvent eventId="TestEvent" topic="TestEvent"/>
        <Component component="test"/>
      </Class>
    </Component>
  </Objects>
</Components>
